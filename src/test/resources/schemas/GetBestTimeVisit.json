{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "BEST_VISIT_ID": {
        "type": "integer"
      },
      "RAYON_ID": {
        "type": "integer"
      },
      "RAYON_CODE": {
        "type": "string"
      },
      "PERSON_ID": {
        "type": "integer"
      },
      "PERSON_CODE": {
        "type": "string"
      },
      "CUSTOMER_ID": {
        "type": "integer"
      },
      "CUSTOMER_NAME": {
        "type": "string"
      },
      "CUSTOMER_LATITUDE": {
        "type": "string",
        "pattern": "^-?\\d+\\.\\d+$"
      },
      "CUSTOMER_LONGITUDE": {
        "type": "string",
        "pattern": "^-?\\d+\\.\\d+$"
      },
      "MONDAY_DAY": {
        "type": "integer",
        "enum": [0, 1]
      },
      "MONDAY_NIGHT": {
        "type": "integer",
        "enum": [0, 1]
      },
      "TUESDAY_DAY": {
        "type": "integer",
        "enum": [0, 1]
      },
      "TUESDAY_NIGHT": {
        "type": "integer",
        "enum": [0, 1]
      },
      "WEDNESDAY_DAY": {
        "type": "integer",
        "enum": [0, 1]
      },
      "WEDNESDAY_NIGHT": {
        "type": "integer",
        "enum": [0, 1]
      },
      "THURSDAY_DAY": {
        "type": "integer",
        "enum": [0, 1]
      },
      "THURSDAY_NIGHT": {
        "type": "integer",
        "enum": [0, 1]
      },
      "FRIDAY_DAY": {
        "type": "integer",
        "enum": [0, 1]
      },
      "FRIDAY_NIGHT": {
        "type": "integer",
        "enum": [0, 1]
      },
      "SCHEDULE_DAY": {
        "type": "string",
        "pattern": "^[01]{7}$"
      },
      "SCHEDULE_NIGHT": {
        "type": "string",
        "pattern": "^[01]{7}$"
      },
      "IS_UPCOUNTRY": {
        "type": "integer",
        "enum": [0, 1]
      }
    },
    "required": [
      "BEST_VISIT_ID",
      "RAYON_ID",
      "RAYON_CODE",
      "PERSON_ID",
      "PERSON_CODE",
      "CUSTOMER_ID",
      "CUSTOMER_NAME",
      "CUSTOMER_LATITUDE",
      "CUSTOMER_LONGITUDE",
      "MONDAY_DAY",
      "MONDAY_NIGHT",
      "TUESDAY_DAY",
      "TUESDAY_NIGHT",
      "WEDNESDAY_DAY",
      "WEDNESDAY_NIGHT",
      "THURSDAY_DAY",
      "THURSDAY_NIGHT",
      "FRIDAY_DAY",
      "FRIDAY_NIGHT",
      "SCHEDULE_DAY",
      "SCHEDULE_NIGHT",
      "IS_UPCOUNTRY"
    ],
    "additionalProperties": false
  }
}
